{"ast":null,"code":"import orderStatistic from \"@/services/data_import.js\";\nimport ApexCharts from \"apexcharts\";\nimport { VirtualScroller } from \"vue-virtual-scroller\";\nexport default {\n  name: \"TheOrderStatistic\",\n  props: {\n    msg: String\n  },\n  data() {\n    return {\n      order_count_list: [],\n      chart: null\n    };\n  },\n  methods: {\n    orderStatlist() {\n      orderStatistic.getOrderStat().then(response => {\n        this.order_count_list = response.data;\n        this.renderChart();\n        console.log(response.data);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    renderChart() {\n      if (this.chart) {\n        this.chart.destroy();\n      }\n      const options = {\n        chart: {\n          type: \"bar\",\n          id: \"vuechart-example\",\n          responsive: true,\n          maintainAspectRatio: false\n        },\n        series: [{\n          name: \"sales\",\n          data: this.order_count_list.map(item => item.order_count)\n        }],\n        xaxis: {\n          categories: this.order_count_list.map(item => item.order_hour)\n        }\n      };\n      this.chart = new ApexCharts(document.querySelector(\"#chart\"), options);\n      this.chart.render();\n    }\n  },\n  watch: {\n    order_count_list() {\n      this.renderChart();\n    }\n  },\n  mounted() {\n    this.orderStatlist();\n  }\n};","map":{"version":3,"names":["orderStatistic","ApexCharts","VirtualScroller","name","props","msg","String","data","order_count_list","chart","methods","orderStatlist","getOrderStat","then","response","renderChart","console","log","catch","e","destroy","options","type","id","responsive","maintainAspectRatio","series","map","item","order_count","xaxis","categories","order_hour","document","querySelector","render","watch","mounted"],"sources":["V:\\web\\shiftfront\\src\\components\\OrderStatistic.vue"],"sourcesContent":["<template>\r\n  <div class=\"Main\">\r\n    <div class=\"AllUsers\">\r\n      Все заказы\r\n      <vue-virtual-scroller :items=\"order_count_list\" :item-height=\"20\">\r\n        <template v-slot=\"{ item, index }\">\r\n          <p>{{ item.order_hour }}, {{ item.order_count }}</p>\r\n        </template>\r\n      </vue-virtual-scroller>\r\n    </div>\r\n    <div id=\"chart\" :style=\"{ height: '300px', width: '100%' }\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport orderStatistic from \"@/services/data_import.js\";\r\nimport ApexCharts from \"apexcharts\";\r\nimport { VirtualScroller } from \"vue-virtual-scroller\";\r\n\r\nexport default {\r\n  name: \"TheOrderStatistic\",\r\n  props: {\r\n    msg: String,\r\n  },\r\n  data() {\r\n    return {\r\n      order_count_list: [],\r\n      chart: null,\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    orderStatlist() {\r\n      orderStatistic\r\n        .getOrderStat()\r\n        .then((response) => {\r\n          this.order_count_list = response.data;\r\n          this.renderChart();\r\n          console.log(response.data);\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    renderChart() {\r\n      if (this.chart) {\r\n        this.chart.destroy();\r\n      }\r\n\r\n      const options = {\r\n        chart: {\r\n          type: \"bar\",\r\n          id: \"vuechart-example\",\r\n          responsive: true,\r\n          maintainAspectRatio: false,\r\n        },\r\n        series: [\r\n          {\r\n            name: \"sales\",\r\n            data: this.order_count_list.map((item) => item.order_count),\r\n          },\r\n        ],\r\n        xaxis: {\r\n          categories: this.order_count_list.map((item) => item.order_hour),\r\n        },\r\n      };\r\n\r\n      this.chart = new ApexCharts(document.querySelector(\"#chart\"), options);\r\n      this.chart.render();\r\n    },\r\n  },\r\n  watch: {\r\n    order_count_list() {\r\n      this.renderChart();\r\n    },\r\n  },\r\n  mounted() {\r\n    this.orderStatlist();\r\n  },\r\n};\r\n</script>